{
  "name": "Extension Demo",
  "version": "1.0",
  "description": "Build an Basic Extension!",
  "permissions": ["activeTab"],

  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "img/sp.png",
      "32": "img/sp.png",
      "48": "img/sp.png"
    }
  },

  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["program.js"]
  }],

  "manifest_version": 2
}
